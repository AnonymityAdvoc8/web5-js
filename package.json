{
  "name": "web5-sdk-js",
  "productName": "web5-sdk-js",
  "version": "0.0.1",
  "description": "SDK for accessing the features and capabilities of Web5",
  "type": "module",
  "main": "./dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "package": "webpack --config webpack.config.js",
    "build": "npm run esbuild:node && npm run esbuild:browser",
    "esbuild:node": "esbuild main.js --bundle --minify --sourcemap --platform=node --format=cjs --target=node16 --outdir=/dist/node",
    "esbuild:browser": "esbuild main.js --bundle --platform=node --format=esm --outdir=/dist/browser --target=esnext"
  },
  "keywords": [],
  "author": {
    "name": "Daniel Buchner",
    "email": "dbuchner@tbd.email"
  },
  "license": "MIT",
  "devDependencies": {
    "source-map-loader": "^4.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@js-temporal/polyfill": "0.4.3",
    "@tbd54566975/dwn-sdk-js": "0.0.22",
    "cross-blob": "3.0.2",
    "cross-fetch": "3.1.5",
    "deepmerge": "4.3.0",
    "esbuild": "0.17.10",
    "ipfs-unixfs-importer": "14.0.1",
    "multiformats": "11.0.0",
    "readable-stream": "4.3.0",
    "tweetnacl": "1.0.3"
  }
}